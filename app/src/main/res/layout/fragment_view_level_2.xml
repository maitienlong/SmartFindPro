<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="presenter"
            type="com.poly.smartfindpro.ui.checklevel.levelbac.RankBacPresenter" />

        <import type="android.view.View" />
    </data>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/_200sdp"
        android:layout_height="@dimen/_200sdp"
        android:layout_gravity="center_horizontal"
        android:src="@mipmap/rank_bac" />

    <TextView
        style="@style/textStyleBold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Thành viên bạc" />

    <TextView
        android:padding="@dimen/_8sdp"
        style="@style/textStyleRegular"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_16sdp"
        android:text="Cho phép xem bài viết, chia sẻ bài viết. Giới hạn đăng bài 3 lượt/ngày" />


</LinearLayout>
</layout>
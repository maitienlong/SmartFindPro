    <div class="page-wrapper">
        <div class="container-fluid">
            <div class="white-box">
                <div style="display: {{status}}" class="alert alert-warning alert-dismissible fade show" role="alert">
                    {{textAlert}}
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <form action="createAdAc" class="container col-8" id="formUD">
                    <input type="text" name="nId" value="{{id}}" class="form-control" id="InputId" required
                           style="display: none">
                    <div class="form-group">
                        <label for="InputUsername">Tên đăng nhập</label>
                        <input type="text" name="nUser" value="{{user}}" class="form-control" id="InputUsername"
                               required
                               placeholder="Tên đăng nhập">
                    </div>
                    <div class="form-group">
                        <label for="InputPassword">Mật khẩu</label>
                        <input name="nPass" value="{{pass}}" class="form-control" id="InputPassword" required
                               placeholder="Mật khẩu">
                    </div>
                    <!--            <div id="ConfirmPassword" class="form-group">-->
                    <!--                <label for="InputConfirmPassword">Nhập lại mật khẩu</label>-->
                    <!--                <input  name="cfPass" class="form-control" id="InputConfirmPassword" required-->
                    <!--                       placeholder="nhập lại mật khẩu">-->
                    <!--                <span id='message'></span>-->
                    <!--            </div>-->
                    <div class="form-group">
                        <label for="InputName">Tên nhân viên</label>
                        <input type="text" name="nName" value="{{name}}" class="form-control" id="InputName" required
                               placeholder="">
                    </div>

                    <div class="form-group">
                        <label for="InputPhone">Số điện thoại</label>
                        <input type="text" name="nPhone" value="{{phone}}" class="form-control" id="InputPhone" required
                               placeholder="">
                    </div>

                    <div class="form-group">
                        <label for="InputAddress">Địa chỉ</label>
                        <input type="text" name="nAddress" value="{{address}}" class="form-control" id="InputAddress"
                               required
                               placeholder="">
                    </div>
                    <br>
                    <div class="row justify-content-center">
                        <div class="col-4">
                            <button type="submit" name="ud" value="1" class="btn btn-outline-dark col-sm-12 ">Cập nhật
                            </button>
                        </div>
                        <div class="col-4">
                            <a href="createAdAc" name="cancel" class="btn btn-outline-dark col-sm-12 ">
                                Hủy tạo
                            </a>
                        </div>
                    </div>
                </form>

<body>

<div class="limiter">
    <div class="container-login100">
        <div class="wrap-login100">
            <div class="login100-form-title" style="background-image: url('public/images/bg-01.jpg');">
					<span class="login100-form-title-1">
						Hệ thống quản trị SmartFind
					</span>
            </div>

            <form class="login100-form validate-form" action="/index" method="get">
                <div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
                    <span class="label-input100">Tài khoản</span>
                    <input value="{{user}}" type="text" name="user" class="input100" id="InputUsername"
                           required
                           placeholder="Nhập tài khoản">
                    <span class="focus-input100"></span>
                </div>

                <div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
                    <span class="label-input100">Mật khẩu</span>
                    <input value="{{pass}}" type="password" name="pass" class="input100" id="InputPassword"
                           required
                           placeholder="Nhập mật khẩu">
                    <span class="focus-input100"></span>
                </div>

                <div class="container-login100-form-btn  p-t-60">
                    <button type="submit" name="sm" value="1" class="login100-form-btn"
                            aria-describedby="lgHelp">Đăng nhập
                    </button>
                </div>
            </form>
            <div class="text-center">
	                <span class="txt1">
                        SmartFindPro&copy;
                    </span>
            </div>
        </div>
    </div>
</div>
